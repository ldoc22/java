public class PangramChecker {
  public boolean check(String sentence){
    char [] c = {'a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i', 'j', 'k', 'l','m', 'n','o', 'p', 
                'q', 'r', 's', 't', 'u', 'v', 'w', 'x', 'y', 'z'};
                sentence = sentence.toLowerCase();
    for(int i = 0; i < c.length;i++){
        if(sentence.indexOf(c[i]) == -1)
            return false;
    }
    return true;
  }
  
}
