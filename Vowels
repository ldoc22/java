public class Vowels {

  public static int getCount(String str) {
    int vowelsCount = 0;
    str = str.toUpperCase();
    for(int i = 0; i < str.length(); i++){
    char x = str.charAt(i);
    if(x == 'A' || x== 'E' || x== 'I' || x== 'O' ||x == 'U')
    vowelsCount++;
    }
    return vowelsCount;
  }

}
